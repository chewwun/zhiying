/* miniprogram/pages/like_page/like_page.wxss */
page{
  height: 100%
}
.allpart{
  height: 100%
}
.tab-part {
  display: flex;
  flex-direction: row;

  margin-bottom: 25rpx;
}
.tab-subject{
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-bottom: 2rpx;
  position: fixed;
  bottom: 0
}
.tab {
  width: 50%;
  text-align: center;
  border-bottom: solid;
  border-bottom-color: #aaa;
  border-bottom-width: 5rpx;
  padding-bottom: 30rpx;
  font-weight: 300;
  color: #aaa;
}
.f{
  display: flex
}
.tab1{
  margin-top:3%; 
  width: 50%;
  text-align: center;
  border-top: solid;
  border-top-color: #aaa;
  border-top-width: 5rpx;
  padding-top: 20rpx;
padding-bottom: 20rpx;
  font-weight: 300;
  color: #aaa;
  
}
.tab1:nth-child(1) {
  border-right: solid;
  border-right-width: 3rpx;
  border-right-color: #aaa;
}
.tab:nth-child(1) {
  border-right: solid;
  border-right-width: 3rpx;
  border-right-color: #aaa;
}

.tab:nth-child(2) {
  border-left: solid;
  border-left-width: 3rpx;
  border-left-color: #aaa;
  border-right: solid;
  border-right-width: 3rpx;
  border-right-color: #aaa;
}
.tab-clicked {
  color: green;
  font-weight: 500;
}

.box{
  font-size:50rpx;
  margin: 5%;
  

  padding: 5%
}
.divLine{
  margin-top: 2%;
  margin-bottom: 2%;
  margin-left: 2%;
 background: #E0E3DA;
 width: 90%;
 height: 0rpx;
 border: 3rpx dashed #a7a8a8;
}
.likelisten{
  font-size:50rpx;
  margin: 5%;
  
  padding: 5%;
  text-overflow:ellipsis;
word-wrap:break-word;
}